{
    "extends": "@react-native/typescript-config/tsconfig.json",
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "jsx": "react-native",
        "lib": [
            "es2017",
            "es2015",
            "dom"
        ],
        "moduleResolution": "bundler",
        "noEmit": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "strict": false
    }
}